{
    "name": "linio/common-symfony",
    "description": "Common library for Linio Symfony projects",
    "keywords": ["linio", "common", "symfony"],
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.1",
        "symfony/form": "^2.8|^3.0",
        "symfony/routing": "^2.8|^3.0",
        "symfony/framework-bundle": "^2.8|^3.0",
        "symfony/security": "^3.2",
        "symfony/templating": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^7.0",
        "friendsofphp/php-cs-fixer": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Linio\\Common\\Symfony\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linio\\Common\\Symfony\\": "tests/"
        }
    }
}
